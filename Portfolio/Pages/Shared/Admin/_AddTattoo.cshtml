@model Portfolio.Pages.Admin.TattooLairModel

<h2> Add New Tattoo</h2>
<div>
    <form asp-page-handler="AddTattoo" method="post" enctype="multipart/form-data">
        <label>
            <input name="files" type="file" multiple="multiple" accept="image/*"/>
            <input type="submit" value="Add Tattoo" />
        </label>
    </form>
</div>